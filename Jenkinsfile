    agent any
    stages {
        stage('stage one') {
            steps {
                script {
                    echo "Stage 1"
                }
               
            }
        }
        stage('stage two') {
            steps {
                echo "Stage 2"
            }
        }
        stage('stage three') {
            
            steps {
                echo "stage 3"
            }
        }
    }
}